# Database Configuration
MYSQL_ROOT_PASSWORD=your_root_password_here
MYSQL_DATABASE=Discogs
MYSQL_USER=discogs_user
MYSQL_PASSWORD=your_database_password_here

# Backend Database Configuration
MY_SQL_DB_HOST=db
MY_SQL_DB_PORT=3306
MY_SQL_DB_USER=discogs_user
MY_SQL_DB_PASSWORD=your_database_password_here
MY_SQL_DB_DATABASE=Discogs
MY_SQL_DB_CONNECTION_LIMIT=10

# Server Configuration
PORT=3001
NODE_ENV=production

# JWT Configuration
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=24h

# Discogs API Configuration
# Get these from https://www.discogs.com/settings/developers
DISCOGS_TOKEN=your_discogs_token_here
DISCOGS_CONSUMER_KEY=your_discogs_consumer_key_here
DISCOGS_CONSUMER_SECRET=your_discogs_consumer_secret_here

# OAuth Configuration
DISCOGS_OAUTH_CALLBACK_URL=http://localhost:3001/api/users/discogs/oauth/callback
FRONTEND_URL=http://localhost:80

# CORS Configuration
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# Frontend Configuration
REACT_APP_API_URL=
